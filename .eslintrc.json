{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "prettier",
        "airbnb-base"
    ],
    "plugins": [
        "prettier"
      ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-shadow": "off",
        "no-underscore-dangle": "off",
    }
}
